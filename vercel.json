{
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
